  "scripts": {
    "pback:serve": "nx serve pback",
    "pback:build": "nx build pback",
    "pback:start": "nx start pback",
    "pmobile:ios": "nx run pmobile:run-ios",
    "pmobile:android": "nx run pmobile:run-android",
    "pmobile:prebuild": "nx run pmobile:prebuild",
    "db:generate": "nx build backend && drizzle-kit generate",
    "db:migrate": "nx build backend && drizzle-kit migrate",
    "ios:back": "nx run-many -p=pback -t=serve -p=pmobile -t=run-ios",
    "android:back": "nx run-many -p=pback -t=serve -p=pmobile -t=run-android",
    "pback:serve:dev": "infisical run --env=dev -- npm run pback:serve",
    "pback:start:dev": "infisical run --env=dev -- npm run pback:start",
    "pback:serve:prod": "infisical run --env=prod -- npm run pback:serve",
    "pback:start:prod": "infisical run --env=prod -- npm run pback:start",
    "pmobile:ios:dev": "infisical run --env=dev -- npm run pmobile:ios",
    "pmobile:android:dev": "infisical run --env=dev -- npm run pmobile:android",
    "pmobile:prebuild:dev": "infisical run --env=dev -- nx run pmobile:prebuild --clean",
    "ios:back:dev": "infisical run --env=dev -- npm run ios:back",
    "android:back:dev": "infisical run --env=dev -- npm run android:back",
    "db:generate:dev": "infisical run --env=dev -- npm run db:generate",
    "db:migrate:dev": "infisical run --env=dev -- npm run db:migrate",
    "db:migrate:test": "infisical run --env=test -- npm run db:migrate",
    "pback:test": "infisical run --env=test -- nx test pback",
    "pmobile:test": "infisical run --env=test -- nx test pmobile",
    "pback:e2e": "infisical run --env=test -- nx e2e pback-e2e",
    "clean": "./tools/scripts/clean.sh",
    "prepare": "husky",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
